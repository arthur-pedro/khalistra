{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@khalistra/shared/(.*)$": "<rootDir>/../../shared/$1",
    "^@khalistra/modules/(.*)$": "<rootDir>/../../modules/$1"
  }
}
